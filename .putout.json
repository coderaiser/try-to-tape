{
    "match": {
        "lib": {
            "remove-console": ["on", {
                "exclude": [
                    "console.error(e)"
                ]
            }]
        },
        "test": {
            "remove-only": "off",
            "remove-skip": "off",
            "remove-unused-variables": ["on", {
                "exclude": [
                    "async (a, b, c) => {}"
                ]
            }]
        }
    }
}
