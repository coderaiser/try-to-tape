{
  "name": "try-to-tape",
  "version": "1.1.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "wrap tape async functions and show error on reject",
  "homepage": "http://github.com/coderaiser/try-to-tape",
  "main": "lib/try-to-tape.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/try-to-tape.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "watch:test": "nodemon -w lib -w test -x \"npm test\"",
    "lint": "eslint lib test",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "currify": "^3.0.0",
    "try-to-catch": "^1.0.2"
  },
  "keywords": [
    "try",
    "catch",
    "function",
    "promise",
    "async",
    "await",
    "try-catch",
    "then",
    "tap",
    "tape"
  ],
  "devDependencies": {
    "@cloudcmd/stub": "^2.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.6.0",
    "nodemon": "^1.14.12",
    "nyc": "^13.0.1",
    "redrun": "^7.0.2",
    "tape": "^4.8.0",
    "try-catch": "^2.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
